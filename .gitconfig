[user]
	name = Camden Webster
	email = camdeniw@gmail.com
	signingkey = /Users/camdenwebster/.ssh/id_ed25519.pub
[core]
	excludesfile = /Users/camdenwebster/.gitignore_global
	editor = nano
[merge]
      tool = vscode
[mergetool "vscode"]
# Comment: Original way before three-way merge shown commented out
#    cmd = code --wait $MERGED
# Comment: For "Three-way merge"
	cmd = code --wait --merge $REMOTE $LOCAL $BASE $MERGED
[diff]
	tool = vscode
[difftool "vscode"]
	cmd = code --wait --diff $LOCAL $REMOTE
[commit]
	template = /Users/camdenwebster/.stCommitMsg
	gpgsign = true
[init]
	templatedir = ~/.git-templates
[gpg]
	format = ssh